{
	"name": "@devprotocol/clubs-plugin-posts-voting",
	"version": "0.6.0",
	"type": "module",
	"description": "Template repository for using TypeScript",
	"main": "dist/index.js",
	"exports": {
		".": {
			"default": "./dist/index.js"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**/*.d.ts",
		"dist/**/*.js",
		"!**/*.test.*",
		"src"
	],
	"keywords": [
		"clubs-plugin"
	],
	"scripts": {
		"test": "ava || echo 'skip'",
		"preview": "astro --root .preview/preview.ssr dev",
		"build": "rollup -c",
		"pretest": "yarn build",
		"prebuild": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.js --fix",
		"lint:format": "prettier --write '**/*.{ts,js,tsx,jsx,astro,vue,svelte,json,md,yml}'",
		"prepack": "yarn test && yarn build",
		"prepare": "husky install"
	},
	"author": "",
	"license": "MPL-2.0",
	"dependencies": {
		"@devprotocol/dev-kit": "8.6.0",
		"@devprotocol/util-ts": "4.0.0",
		"bignumber.js": "9.1.2",
		"ethers": "6.11.1",
		"ramda": "0.29.1",
		"sass": "1.72.0",
		"uuid": "^9.0.1"
	},
	"resolutions": {
		"@devprotocol/util-ts": "4.0.0"
	},
	"peerDependencies": {
		"@devprotocol/clubs-core": "^2.1.1 || ^3.0.0",
		"@devprotocol/clubs-plugin-posts": "^0.7.0 || ^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^0.14.0 || ^0.15.0"
	},
	"devDependencies": {
		"@astrojs/svelte": "^5.0.0",
		"@astrojs/tailwind": "^5.0.0",
		"@astrojs/vue": "^4.0.0",
		"@ava/typescript": "4.1.0",
		"@devprotocol/clubs-core": "3.8.0",
		"@devprotocol/clubs-plugin-posts": "^0.15.3",
		"@rollup/plugin-typescript": "11.1.6",
		"@tailwindcss/typography": "^0.5.10",
		"@types/dotenv": "8.2.0",
		"@types/node": "20.11.30",
		"@types/ramda": "0.29.11",
		"@typescript-eslint/eslint-plugin": "7.3.1",
		"@typescript-eslint/parser": "7.3.1",
		"astro": "^4.0.0",
		"ava": "6.1.2",
		"dotenv": "16.4.5",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-functional": "6.2.0",
		"husky": "9.0.11",
		"prettier": "3.2.5",
		"prettier-plugin-astro": "^0.13.0",
		"prettier-plugin-svelte": "^3.0.0",
		"rimraf": "5.0.5",
		"rollup": "4.13.0",
		"svelte": "^4.2.2",
		"tailwindcss": "3.4.1",
		"typescript": "5.4.3",
		"vue": "3.4.21"
	},
	"ava": {
		"files": [
			"**/*.test.ts"
		],
		"typescript": {
			"rewritePaths": {
				"/": "dist/"
			},
			"compile": false
		}
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/dev-protocol/clubs-plugin-posts-voting.git"
